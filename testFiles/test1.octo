pipeline main(one: vec4, two: vec4, three: vec4) -> (vec4)
    with {
        camera_pos: vec3,
        light_color: vec3,
        light_pos: vec3,
        roman_pos: vec2,
        wutss: mat4,
        wotc1: mat3,
    }
{
    let wut = sin(camera_pos);
    let by = vec2(0.01, 0.01);
    let som = by * by + by;
    let somens = wut;

    if (som.x > by.y) {
        by.x = 12.0;
    }

    for(let i = 0; i<2; i = i + 1) {
        wut.x = wut.x + light_pos.x;
        wut.x = shift(wut.x * 2.0, by);
    }

    return vec4(wut.x, wut.y, wut.z, 1.0);
}
